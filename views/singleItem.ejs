
<% users.forEach(user => { %>
    <div class="row">
        <div class="col s4 m4">
          <div class="card">
            <div class="card-image">
                <img src="<%= user.profile %>" alt="profile-img">
            </div>
            <div class="card-content">
                <h2><%= user.firstname %></h2>
            </div>
          </div>
        </div>
      </div>
<% }) %>

